set nocompatible
set autoindent
set smartindent
set cindent
set tabstop=2
set shiftwidth=2
set showmatch
"set expandtab
set number
set nowrap
set textwidth=200
set statusline=%F%m%r%h%w\ %=[ASCII=\%03.3b]\ [HEX=\%02.2B]\ [POS=%04l,%04v][%p%%]\ [LEN=%L]
set laststatus=2 
set nocp
filetype plugin on
colorscheme desert
